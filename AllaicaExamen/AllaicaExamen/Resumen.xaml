<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AllaicaExamen.Resumen">
    <ContentPage.Content>
        <StackLayout>

            <Label Text="RESUMEN DE INSCRIPCION" TextColor="Black"/>
            <Label x:Name="lblNombre" TextColor="Blue"/>
            <Label x:Name="lblPagoMensual" TextColor="Blue"/>
           <Entry x:Name="txtNombre" IsReadOnly="True"/>
            <Entry x:Name="txtPagoMensual" IsReadOnly="True"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>